{
  "name": "kenotejs-use-koa",
  "version": "1.0.0",
  "description": "Using Koa web framework in Kenote.js",
  "private": true,
  "typings": "./types",
  "license": "MIT",
  "dependencies": {
    "@kenote/config": "latest",
    "@kenote/core": "latest",
    "@kenote/koa": "latest",
    "@kenote/koa-passport": "latest",
    "@kenote/koa-session": "latest",
    "apollo-server-koa": "^2.25.3",
    "class-validator": "^0.13.2",
    "connect-redis": "^6.1.1",
    "graphql": "^15.8.0",
    "koa-redis": "^4.0.1",
    "log4js": "^6.4.1",
    "nunjucks": "^3.2.3",
    "parse-string": "^1.2.4",
    "passport-jwt": "^4.0.0",
    "ioredis": "^4.28.5",
    "type-graphql": "^1.1.1"
  },
  "devDependencies": {
    "@types/bytes": "^3.1.1",
    "@types/lodash": "^4.14.178",
    "@types/node": "^14.18.12",
    "@types/passport-jwt": "^3.0.6",
    "cross-env": "^7.0.3",
    "eslint": "^8.9.0",
    "eslint-config-standard-ts": "^21.0.0",
    "nodemon": "^2.0.15",
    "ts-node": "^10.5.0",
    "tsconfig-paths": "^3.12.0",
    "tscpaths": "^0.0.9",
    "typescript": "^4.5.5"
  },
  "scripts": {
    "dev": "nodemon --config nodemon.json ./src/index.ts",
    "build": "tsc --project src/tsconfig.json && tscpaths -p src/tsconfig.json -s ./src -o ./dist",
    "start": "[ -f ./dist/index.js ] && cross-env NODE_ENV=production node ./dist/index.js",
    "lint": "eslint . --ext .js,.ts, --cache",
    "lint:fix": "eslint . --ext .js,.ts --cache --fix"
  },
  "engines": {
    "node": ">=14"
  }
}
